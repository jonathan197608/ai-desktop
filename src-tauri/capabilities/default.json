  "permissions": [
    "core:default",
    "os:default",
    "log:default",
    "fs:default",
    "fs:read-files",
    "fs:scope-appdata",
    "fs:allow-appdata-meta-recursive",
    "fs:allow-appdata-write-recursive",
    "fs:allow-read-text-file",
    "fs:allow-resource-read-recursive",
    "dialog:default",
    "store:default",
    "clipboard-manager:allow-write-image",
